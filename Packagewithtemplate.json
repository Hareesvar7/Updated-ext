{
    "name": "my-vscode-extension",
    "displayName": "My VS Code Extension",
    "description": "An extension to assist with cloud storage templates and OPA validation.",
    "version": "0.0.1",
    "publisher": "your-name",
    "engines": {
        "vscode": "^1.50.0"
    },
    "activationEvents": [
        "onCommand:extension.generateStorageTemplate"
    ],
    "main": "./src/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "extension.generateStorageTemplate",
                "title": "Generate Storage Template"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./out/install.js"
    },
    "devDependencies": {
        "typescript": "^4.0.0",
        "vscode": "^1.1.37"
    }
}
